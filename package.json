    "async-es": "^2.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "coffee-script": "~1.8.0",
    "cookiejar": "^2.1.1",
    "csprng": "^0.1.2",
    "dotenv": "^4.0.0",
    "gists": "^0.1.2",
    "github-api": "^3.0.0",
    "github-base": "^0.5.4",
    "google-spreadsheet": "^2.0.3",
    "hubot": "^2.19.0",
    "hubot-gitter2": "^0.1.3",
    "hubot-google-images": "^0.2.6",
    "hubot-heroku-keepalive": "^1.0.2",
    "hubot-maps": "0.0.2",
    "hubot-motivate": "^3.0.0",
    "hubot-redis-brain": "0.0.3",
    "hubot-rules": "^0.1.2",
    "hubot-scripts": "^2.17.2",
    "hubot-shipit": "^0.2.0",
    "hubot-timezone": "^1.0.1",
    "moment": "^2.18.1",
    "node-gitter": "2.0.5",
    "path": "^0.12.7",
    "xo": "^0.18.2"
  },
  "engines": {
    "node": "0.10.x"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "hubot-test-helper": "^1.5.1",
    "mocha": "^3.2.0",
    "sinon": "^2.1.0"
  },
  "scripts": {
    "test": "xo --quiet && mocha --require coffee-script/register --compilers coffee:coffee-script --compilers js:babel-core/register test/*.js",
    "watch:test": "npm t -- --watch",
    "lint": "xo --quiet"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "no-use-before-define": 1,
      "no-await-in-loop": 1,
      "import/prefer-default-export": 1,
      "no-negated-condition": 1,
      "guard-for-in": 1
    },
    "ignores": [
      "scripts/000-import-scripts.js"
    ]
  }
}
